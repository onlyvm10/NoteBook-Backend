{
   "name": "your-project-name",
   "version": 2,
   "builds": [
      {
         "src": "Backend/index.js",
         "use": "@vercel/node"
      },
      {
         "src": "Frontend/build",
         "use": "@vercel/static"
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "Backend/index.js"
      },
      {
         "handle": "filesystem"
      },
      {
         "src": "/(.*)",
         "dest": "Frontend/build/$1"
      }
   ]
}
